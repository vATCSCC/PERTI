{
	"info": {
		"_postman_id": "swim-api-v1-collection",
		"name": "VATSIM SWIM API v1",
		"description": "System Wide Information Management (SWIM) API for VATSIM.\n\nSWIM provides a centralized data exchange hub enabling real-time flight information sharing across the VATSIM ecosystem.\n\n## Authentication\n\nAll endpoints require Bearer token authentication. Set your API key in the collection variables or environment.\n\n## API Key Tiers\n\n| Tier | Prefix | Rate Limit | Write Access |\n|------|--------|------------|-------------|\n| System | `swim_sys_` | 10,000/min | Full |\n| Partner | `swim_par_` | 1,000/min | Limited |\n| Developer | `swim_dev_` | 100/min | Read-only |\n| Public | `swim_pub_` | 30/min | Read-only |\n\n## Documentation\n\nhttps://perti.vatcscc.org/docs/swim/",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{api_key}}",
				"type": "string"
			}
		]
	},
	"variable": [
		{
			"key": "base_url",
			"value": "https://perti.vatcscc.org/api/swim/v1",
			"type": "string"
		},
		{
			"key": "api_key",
			"value": "swim_dev_your_key_here",
			"type": "string"
		}
	],
	"item": [
		{
			"name": "Info",
			"item": [
				{
					"name": "Get API Info",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/",
							"host": ["{{base_url}}"],
							"path": [""]
						},
						"description": "Returns API version, available endpoints, and authentication information."
					},
					"response": []
				}
			],
			"description": "API information and status"
		},
		{
			"name": "Flights",
			"item": [
				{
					"name": "List Flights",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flights?status=active&per_page=100&page=1",
							"host": ["{{base_url}}"],
							"path": ["flights"],
							"query": [
								{
									"key": "status",
									"value": "active",
									"description": "Filter by status: active, completed, all"
								},
								{
									"key": "dept_icao",
									"value": "KJFK,KEWR",
									"description": "Filter by departure airport(s)",
									"disabled": true
								},
								{
									"key": "dest_icao",
									"value": "KLAX,KSFO",
									"description": "Filter by destination airport(s)",
									"disabled": true
								},
								{
									"key": "artcc",
									"value": "ZNY,ZLA",
									"description": "Filter by destination ARTCC(s)",
									"disabled": true
								},
								{
									"key": "callsign",
									"value": "UAL*",
									"description": "Filter by callsign pattern (* wildcard)",
									"disabled": true
								},
								{
									"key": "tmi_controlled",
									"value": "true",
									"description": "Filter to TMI-controlled flights only",
									"disabled": true
								},
								{
									"key": "phase",
									"value": "ENROUTE,DESCENT",
									"description": "Filter by flight phase(s)",
									"disabled": true
								},
								{
									"key": "per_page",
									"value": "100",
									"description": "Results per page (1-1000)"
								},
								{
									"key": "page",
									"value": "1",
									"description": "Page number"
								}
							]
						},
						"description": "Returns a paginated list of flights with optional filters.\n\nData is sourced from the SWIM_API database which syncs from VATSIM_ADL every 2 minutes."
					},
					"response": []
				},
				{
					"name": "List Flights - By Departure",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flights?dept_icao=KJFK,KEWR,KLGA&status=active",
							"host": ["{{base_url}}"],
							"path": ["flights"],
							"query": [
								{
									"key": "dept_icao",
									"value": "KJFK,KEWR,KLGA"
								},
								{
									"key": "status",
									"value": "active"
								}
							]
						},
						"description": "Example: Get all active flights departing from New York area airports."
					},
					"response": []
				},
				{
					"name": "List Flights - By Destination ARTCC",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flights?artcc=ZLA&status=active",
							"host": ["{{base_url}}"],
							"path": ["flights"],
							"query": [
								{
									"key": "artcc",
									"value": "ZLA"
								},
								{
									"key": "status",
									"value": "active"
								}
							]
						},
						"description": "Example: Get all active flights destined for Los Angeles Center airspace."
					},
					"response": []
				},
				{
					"name": "List Flights - TMI Controlled Only",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flights?tmi_controlled=true&status=active",
							"host": ["{{base_url}}"],
							"path": ["flights"],
							"query": [
								{
									"key": "tmi_controlled",
									"value": "true"
								},
								{
									"key": "status",
									"value": "active"
								}
							]
						},
						"description": "Example: Get all flights currently under TMI control (Ground Stop, GDP, etc.)."
					},
					"response": []
				},
				{
					"name": "Get Single Flight - By GUFI",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flight?gufi=VAT-20260115-UAL123-KJFK-KLAX",
							"host": ["{{base_url}}"],
							"path": ["flight"],
							"query": [
								{
									"key": "gufi",
									"value": "VAT-20260115-UAL123-KJFK-KLAX",
									"description": "GUFI format: VAT-YYYYMMDD-CALLSIGN-DEPT-DEST"
								},
								{
									"key": "include_history",
									"value": "false",
									"description": "Include inactive flights in search",
									"disabled": true
								}
							]
						},
						"description": "Returns detailed information for a single flight by GUFI.\n\nGUFI format: `VAT-YYYYMMDD-CALLSIGN-DEPT-DEST`"
					},
					"response": []
				},
				{
					"name": "Get Single Flight - By flight_uid",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flight?flight_uid=123456",
							"host": ["{{base_url}}"],
							"path": ["flight"],
							"query": [
								{
									"key": "flight_uid",
									"value": "123456",
									"description": "Internal flight unique identifier"
								}
							]
						},
						"description": "Returns detailed information for a single flight by internal flight_uid."
					},
					"response": []
				},
				{
					"name": "Get Single Flight - By flight_key",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/flight?flight_key=UAL123_KJFK_KLAX_20260115",
							"host": ["{{base_url}}"],
							"path": ["flight"],
							"query": [
								{
									"key": "flight_key",
									"value": "UAL123_KJFK_KLAX_20260115",
									"description": "Flight key string"
								}
							]
						},
						"description": "Returns detailed information for a single flight by flight_key."
					},
					"response": []
				}
			],
			"description": "Flight data retrieval endpoints"
		},
