{
  "name": "@vatsim/swim-client",
  "version": "1.0.0",
  "description": "VATSWIM API Client - REST API and WebSocket client for flight data",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vatsim",
    "swim",
    "aviation",
    "websocket",
    "flight-tracking",
    "api-client"
  ],
  "author": "vATCSCC <dev@vatcscc.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vatcscc/swim-client-js.git"
  },
  "homepage": "https://perti.vatcscc.org",
  "bugs": {
    "url": "https://github.com/vatcscc/swim-client-js/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  }
}
