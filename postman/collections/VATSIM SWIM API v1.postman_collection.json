              ],
              "path": [
                "flights"
              ],
              "query": [
                {
                  "key": "status",
                  "value": "active",
                  "description": "Filter by status: active, completed, all"
                },
                {
                  "key": "dept_icao",
                  "value": "KJFK,KEWR",
                  "description": "Filter by departure airport(s)",
                  "disabled": true
                },
                {
                  "key": "dest_icao",
                  "value": "KLAX,KSFO",
                  "description": "Filter by destination airport(s)",
                  "disabled": true
                },
                {
                  "key": "artcc",
                  "value": "ZNY,ZLA",
                  "description": "Filter by destination ARTCC(s)",
                  "disabled": true
                },
                {
                  "key": "callsign",
                  "value": "UAL*",
                  "description": "Filter by callsign pattern (* wildcard)",
                  "disabled": true
                },
                {
                  "key": "tmi_controlled",
                  "value": "true",
                  "description": "Filter to TMI-controlled flights only",
                  "disabled": true
                },
                {
                  "key": "phase",
                  "value": "ENROUTE,DESCENT",
                  "description": "Filter by flight phase(s)",
                  "disabled": true
                },
                {
                  "key": "per_page",
                  "value": "100",
                  "description": "Results per page (1-1000)"
                },
                {
                  "key": "page",
                  "value": "1",
                  "description": "Page number"
                }
