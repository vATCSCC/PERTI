        {
          "name": "Push Telemetry (Position Update)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"flights\": [{\n        \"callsign\": \"DAL123\",\n        \"dept_icao\": \"KJFK\",\n        \"dest_icao\": \"KLAX\",\n        \"phase\": \"ENROUTE\",\n        \"latitude\": 38.5432,\n        \"longitude\": -95.1234,\n        \"altitude_ft\": 37000,\n        \"heading_deg\": 270,\n        \"groundspeed_kts\": 485,\n        \"vertical_rate_fpm\": 0\n    }]\n}"
