{
    "name": "vatsim/swim-client",
    "description": "VATSWIM API client library for PHP - Flight data exchange for virtual airlines",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "VATCSCC Development Team",
            "email": "dev@vatcscc.org"
        }
    ],
    "homepage": "https://perti.vatcscc.org/swim/docs",
    "keywords": [
        "vatsim",
        "swim",
        "flight-data",
        "virtual-airline",
        "aviation"
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "VatSim\\Swim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VatSim\\Swim\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=6"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
