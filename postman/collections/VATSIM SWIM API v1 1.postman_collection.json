              "mode": "raw",
              "raw": "{\n    \"flights\": [\n        {\n            \"callsign\": \"UAL123\",\n            \"dept_icao\": \"KJFK\",\n            \"dest_icao\": \"KLAX\",\n            \"cid\": 1234567,\n            \"aircraft_type\": \"B738\",\n            \"route\": \"DEEZZ5 DEEZZ J584 ORD J64 RIIVR2\",\n            \"current_phase\": \"ENROUTE\",\n            \"is_active\": true,\n            \"tmi\": {\n                \"ctl_type\": \"GDP\",\n                \"slot_time_utc\": \"2026-01-15T18:30:00Z\",\n                \"delay_minutes\": 45\n            }\n        }\n    ]\n}",
